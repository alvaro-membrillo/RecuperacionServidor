<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Practica1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Inicio</servlet-name>
    <servlet-class>org.iesalixar.servidor.controller.MainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Inicio</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Reservar</servlet-name>
    <servlet-class>org.iesalixar.servidor.controller.ReservaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Reservar</servlet-name>
    <url-pattern>/Reservar</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>org.iesalixar.servidor.controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Confirmar</servlet-name>
    <servlet-class>org.iesalixar.servidor.controller.ConfirmarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Confirmar</servlet-name>
    <url-pattern>/Confirmar</url-pattern>
  </servlet-mapping>
</web-app>